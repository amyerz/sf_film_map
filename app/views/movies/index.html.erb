<h1>Listing Movies</h1>

<table>
  <tr>
    <th>Movie Title</th>
    <th>Released Year</th>
    <th>Director</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @movies.each do |movie| %>
  <tr>
    <td><%= movie.movie_title %></td>
    <td><%= movie.release_year %></td>
    <td><%= movie.director %></td>
    <td><%= link_to 'Show', movie_path(movie) %></td>
  </tr>
<% end %>
</table>

<br />

